{
  "name": "finalProject_client",
  "version": "0.1.0",
  "description": "UNO - shop",
  "engines": {
    "node": "8"
  },
  "main": "./server/server.js",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "colors": "^1.3.3",
    "concurrently": "^4.1.0",
    "connect-ensure-login": "^0.1.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "http-errors": "~1.6.2",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.3.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "password-meter": "^3.1.0",
    "password-validator": "^4.1.1",
    "query-string": "^6.4.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.5.0",
    "react-image-magnify": "^2.7.4",
    "react-loader-spinner": "^2.3.0",
    "react-map-gl": "^4.1.1",
    "react-mapbox-gl": "^4.2.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "react-slick": "^0.23.2",
    "react-transition-group": "^2.7.1",
    "redux": "^4.0.1",
    "redux-form": "^8.1.0",
    "redux-form-input-masks": "^2.0.1",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "session-file-store": "^1.2.0",
    "slick-carousel": "^1.8.1",
    "slugify": "^1.3.4",
    "uuid": "^3.3.2",
    "validator": "^10.11.0"
  },
  "scripts": {
    "start": "node ./server/server.js",
    "build": "npm run stylelint-build && react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "stylelint-dev": "stylelint 'src/**/*.scss' --cache --cache-location './.stylelintcache'; exit 0",
    "stylelint-build": "stylelint 'src/**/*.scss'",
    "stylelint-fix-sass": "stylelint --fix 'src/**/*.scss'",
    "start-dev": "concurrently \"npm run client\" \"npm run server\"",
    "start-react": "react-scripts start",
    "server-start": "concurrently \"npm run client\" \"npm run server\"",
    "server-test-one:win": "set NODE_ENV=test&& node_modules\\.bin\\mocha  ./server/test/services/mail.test.js --reporter spec --exit",
    "server-test:win": "set NODE_ENV=test&& node_modules\\.bin\\mocha  ./server/test/**/*.test.js --reporter spec --exit",
    "server-test:lin": "NODE_ENV=test ./node_modules/.bin/mocha ./server/test/**/*.test.js --reporter spec --exit",
    "server": "nodemon --ignore ./sessions ./server/server.js -L",
    "client": "npm-run-all -p watch-styles-sass start-react",
    "client-test": "react-scripts test",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha ./server/test/**/*.test.js --recursive --reporter spec --exit",
    "watch-styles-sass": "npm-watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-date-string": "^0.1.0",
    "chai-http": "^4.2.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.10",
    "prop-types": "^15.6.2",
    "sinon": "^7.2.3",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.7.0",
    "supertest": "^3.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pasajero0/finalProject_client"
  },
  "watch": {
    "stylelint-dev": {
      "patterns": [
        "/src/**/*.scss"
      ],
      "extensions": "scss"
    }
  }
}
